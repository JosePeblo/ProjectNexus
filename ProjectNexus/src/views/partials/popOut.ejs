<% if(error=='You do not have unassigned epics' ) { %>
    <div
        class="content-center mx-auto grid-rows-3 grid-flow-col justify-items-center bg-gray-400 rounded-xl gap-8 w-3/12 text-center">
        <div class="text-zinc-200">
            Please click the button to procede
        </div>
        <button onclick="removePopout(event)" id="popOut"
            class="rounded-xl bg-lime-500 justify-items-center text-zinc-100">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-check"
                viewBox="0 0 16 16">
                <path
                    d="M10.97 4.97a.75.75 0 0 1 1.07 1.05l-3.99 4.99a.75.75 0 0 1-1.08.02L4.324 8.384a.75.75 0 1 1 1.06-1.06l2.094 2.093 3.473-4.425a.267.267 0 0 1 .02-.022z" />
            </svg>
        </button>
        <div class="rounded-xl">
            <%= error%>
        </div>
    </div>
    <% } %>